Imports RS_EquipSimulation.Common.Singleton

Namespace MasterSystem.Creature


	Public Class CreaturePassiveEffectSingletonModel
		Inherits SingletonConstantBase(Of CreaturePassiveEffectSingletonModel)

		' マスターデータの保持
		Private _passiveEffects As List(Of CreaturePassiveEffectModel)

		' 読み出し用変数
		Private _currentPassiveEffect As CreaturePassiveEffectModel = Nothing
		Private _currentEffectName As String = Nothing
		Private _currentValues As New List(Of String)()

		' CSVデータの定義
		Protected Overrides ReadOnly Property ConstantData As String
			Get
				Return PassiveEffectData
			End Get
		End Property

		Protected Overrides Sub SafeInitialize()
			_passiveEffects = New List(Of CreaturePassiveEffectModel)
			_currentPassiveEffect = Nothing
			_currentEffectName = Nothing
			_currentValues = New List(Of String)()
		End Sub

		' データの読み込み処理
		Protected Overrides Sub ProcessLine(parts() As String)
			_currentEffectName = parts(2)
			_currentValues = parts.Skip(3).Select(Function(v) v).ToList()

			' 最初の列に値がある場合（新しいパッシブエフェクト）
			If Not String.IsNullOrWhiteSpace(parts(0)) Then
				' 前のデータを処理
				ProcessCompleteEffect()

				' 新しいパッシブエフェクトの作成
				_currentPassiveEffect = New CreaturePassiveEffectModel(parts(0), parts(1))
			End If

			' 効果名の処理
			ProcessCurrentEffect()
		End Sub

		Private Sub ProcessCurrentEffect()
			If _currentPassiveEffect IsNot Nothing AndAlso
		   Not String.IsNullOrEmpty(_currentEffectName) AndAlso
		   _currentValues.Count > 0 Then
				_currentPassiveEffect.AddEffect(_currentEffectName, _currentValues)
			End If
		End Sub

		Private Sub ProcessCompleteEffect()
			ProcessCurrentEffect()
			If _currentPassiveEffect IsNot Nothing Then
				_passiveEffects.Add(_currentPassiveEffect)
			End If
		End Sub

#Region "公開メソッド"
		' パッシブ効果の取得
		Public Function GetPassiveEffect(passiveName As String) As CreaturePassiveEffectModel
			Return _passiveEffects.FirstOrDefault(Function(p) p.Name = passiveName)
		End Function

		' すべてのパッシブ効果を取得
		Public Function GetAllPassiveEffects() As List(Of CreaturePassiveEffectModel)
			Return _passiveEffects.ToList()
		End Function
#End Region

#Region "検証メソッド"
		' パッシブ名の存在チェック
		Public Function IsValidPassiveName(passiveName As String) As Boolean
			Return _passiveEffects.Any(Function(p) p.Name = passiveName)
		End Function

		' 効果名の存在チェック
		Public Function IsValidEffectName(passiveName As String, effectName As String) As Boolean
			Dim effect = GetPassiveEffect(passiveName)
			Return effect IsNot Nothing AndAlso effect.Effects.ContainsKey(effectName)
		End Function

		' レベル値の検証
		Public Function IsValidLevel(passiveName As String, level As Integer) As Boolean
			Dim effect = GetPassiveEffect(passiveName)
			If effect Is Nothing Then Return False

			Return level >= 0 AndAlso level <= 50
		End Function
#End Region

	End Class

